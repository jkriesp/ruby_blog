<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
  <li>
    <%= link_to tag.name, tag_path(tag), class: "tag_title", id: "tag_#{tag.id}"%>
    <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Are you sure?"}%>
  </li>
  <% end %>
</ul>
<hr>
<%= link_to "Browse Tags", tags_path %></br>
<%= link_to "<< Back to Articles List", articles_path %>